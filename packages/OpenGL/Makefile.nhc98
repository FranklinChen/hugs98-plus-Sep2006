# Extremely poor replacement for autoconf...
$(OBJDIR)/Graphics/Rendering/OpenGL/GL/BasicTypes.$O: include/config.h

include/config.h:
	$(RM) $@
	@echo "Creating $@ ..."
	@echo "#ifndef __FPTOOLS_CONFIG_H__"    >  $@
	@echo "#define __FPTOOLS_CONFIG_H__"    >> $@
	@echo "#define HTYPE_GLBITFIELD Word32" >> $@
	@echo "#define HTYPE_GLBOOLEAN Word8"   >> $@
	@echo "#define HTYPE_GLBYTE Int8"       >> $@
	@echo "#define HTYPE_GLCLAMPD Double"   >> $@
	@echo "#define HTYPE_GLCLAMPF Float"    >> $@
	@echo "#define HTYPE_GLDOUBLE Double"   >> $@
	@echo "#define HTYPE_GLENUM Word32"     >> $@
	@echo "#define HTYPE_GLFLOAT Float"     >> $@
	@echo "#define HTYPE_GLINT Int32"       >> $@
	@echo "#define HTYPE_GLSHORT Int16"     >> $@
	@echo "#define HTYPE_GLSIZEI Int32"     >> $@
	@echo "#define HTYPE_GLUBYTE Word8"     >> $@
	@echo "#define HTYPE_GLUINT Word32"     >> $@
	@echo "#define HTYPE_GLUSHORT Word16"   >> $@
	@echo "#endif" >> $@

